    "@trpc/react-query": "^10.45.2",
    "@vscode_frontend/backend": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "reset-css": "^5.0.2",
    "sass": "^1.83.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.14.0",
    "include-media": "^2.0.0",
    "prettier": "^3.4.2",
    "vite": "^6.0.5"
