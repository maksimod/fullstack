{
  "name": "vscode_frontend",
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^9.1.7",
    "i": "^0.3.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.7.3",
    "typescript-transform-paths": "^3.5.3"
  },
  "scripts": {
    "b": "pnpm --filter @vscode_frontend/backend",
    "w": "pnpm --filter @vscode_frontend/webapp",
    "dev": "pnpm -r --parallel dev",
    "types": "concurrently 'pnpm w types' 'pnpm b types' --noEmit",
    "prettify": "pnpm -r --parallel prettify",
    "lint": "concurrently 'pnpm b lint' 'pnpm w lint'",
    "lint:fix": "concurrently 'pnpm b lint:fix' 'pnpm w lint:fix'",
    "prepare": "husky"
  }
}
