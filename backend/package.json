{
  "name": "@vscode_frontend/backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node-dev --watch ./src/emails/dist,./.env --rs --exit-child ./src/index.ts",
    "build-emails": "rimraf ./src/emails/dist && mkdir ./src/emails/dist && mjml ./src/emails/[!_]*.mjml -o ./src/emails/dist",
    "watch-emails": "mjml -w ./src/emails/[!_]*.mjml -o ./src/emails/dist",
    "copy-emails": "copyfiles ./src/emails/dist/*.html ./dist/backend",
    "build-ts": "rimraf ./dist && tsc --build ./tsconfig.json",
    "build": "pnpm build-ts && pnpm build-emails && pnpm copy-emails",
    "start": "node ./dist/backend/src/index.js",
    "pmd": "prisma migrate dev",
    "pgc": "prisma generate",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts .",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "prepare": "pnpm pgc && ts-patch install -s"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/cron": "^2.4.3",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.5",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/triple-beam": "1.3.3",
    "eslint-plugin-node": "^11.1.0",
    "prisma": "^6.2.1"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@trpc/server": "^10.45.2",
    "@vscode_frontend/shared": "workspace:*",
    "@vscode_frontend/webapp": "workspace:*",
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "cron": "^4.0.0",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fast-glob": "^3.3.3",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mjml": "4.14.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "picocolors": "1.0.0",
    "react-router-dom": "^6.15.0",
    "serialize-error": "8.1.0",
    "superjson": "1.12.2",
    "triple-beam": "1.4.1",
    "trpc-playground": "^1.0.4",
    "winston": "3.10.0",
    "yaml": "2.3.2",
    "zod": "^3.24.1"
  }
}
